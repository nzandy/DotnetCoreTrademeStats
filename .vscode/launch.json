{
	"version": "0.2.0",
	"configurations": [

		{
			"name": "Rental Listing Scanner.",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceRoot}/ListingScanner/bin/Debug/netcoreapp1.1/ListingScanner.dll",
			"args": [],
			"cwd": "${workspaceRoot}/ListingScanner",
			"stopAtEntry": false,
			"externalConsole": false
		},
		{
			"name": "Locality Fetcher",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceRoot}/Utilities/FetchTrademeLocalities/bin/Debug/netcoreapp1.1/FetchTrademeLocalities.dll",
			"args": [],
			"cwd": "${workspaceRoot}/Utilities/FetchTrademeLocalities",
			"stopAtEntry": false,
			"externalConsole": false
		},
		{
			"name": ".NET Core Launch (web)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceRoot}/bin/Debug/<target-framework>/<project-name.dll>",
			"args": [],
			"cwd": "${workspaceRoot}",
			"stopAtEntry": false,
			"launchBrowser": {
				"enabled": true,
				"args": "${auto-detect-url}",
				"windows": {
					"command": "cmd.exe",
					"args": "/C start ${auto-detect-url}"
				},
				"osx": {
					"command": "open"
				},
				"linux": {
					"command": "xdg-open"
				}
			},
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"sourceFileMap": {
				"/Views": "${workspaceRoot}/Views"
			}
		},
		{
			"name": ".NET Core Attach",
			"type": "coreclr",
			"request": "attach",
			"processId": "${command.pickProcess}"
		}
	]
}